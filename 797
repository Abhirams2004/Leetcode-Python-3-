class Solution:
    def allPathsSourceTarget(self, graph: list[list[int]]) -> list[list[int]]:
        def dfs(u):
            if u == n - 1:
                ans.append(path[:])
                return
            for v in graph[u]:
                path.append(v)
                dfs(v)
                path.pop()
        n = len(graph)
        ans = []
        path = [0]
        dfs(0)
        return ans
